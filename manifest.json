{
    "manifest_version": 2,
    "name": "MD new page",
    "version":  "0.1",
    "chrome_url_overrides": {
        "newtab": "newpage.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "web_accessible_resources": ["script.js", "lib/markdown-js-0.5.0/lib/markdown.js"],
    "permissions": [
        "storage",
        {"fileSystem": ["write", "retainEntries", "directory"]}
    ]
}
